{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": [
      1,
      "never"
    ],
    "no-cond-assign": [
      1,
      "always"
    ],
    "no-console": 0,
    "no-constant-condition": 0,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 0,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 0,
    "no-extra-semi": 1,
    "no-func-assign": 2,
    "no-inner-declarations": [
      1,
      "functions"
    ],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 2,
    "no-unreachable": 1,
    "use-isnan": 1,
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,
    "accessor-pairs": [
      2,
      {
        "setWithoutGet": true
      }
    ],
    "block-scoped-var": 1,
    "complexity": [
      1,
      4
    ],
    "consistent-return": 0,
    "curly": [
      2,
      "all"
    ],
    "default-case": 1,
    "dot-notation": 1,
    "dot-location": [
      1,
      "property"
    ],
    "eqeqeq": 2,
    "guard-for-in": 1,
    "no-alert": 0,
    "no-caller": 2,
    "no-div-regex": 2,
    "no-else-return": 1,
    "no-eq-null": 2,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 1,
    "no-implied-eval": 1,
    "no-invalid-this": 1,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 1,
    "no-loop-func": 2,
    "no-multi-spaces": 1,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 1,
    "no-proto": 2,
    "no-redeclare": 1,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 0,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 2,
    "no-useless-call": 2,
    "no-void": 2,
    "no-warning-comments": 0,
    "no-with": 2,
    "radix": 1,
    "vars-on-top": 0,
    "wrap-iife": [
      1,
      "any"
    ],
    "yoda": [
      2,
      "never",
      {}
    ],
    "strict": [
      2,
      "global"
    ],
    "init-declarations": 0,
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": [
      1,
      {
        "builtinGlobals": true,
        "hoist": "all",
        "allow": [
          "resolve",
          "reject",
          "err"
        ]
      }
    ],
    "no-undef-init": 0,
    "no-undef": 0,
    "no-undefined": 2,
    "no-unused-vars": 0,
    "no-use-before-define": 0,
    "callback-return": 1,
    "handle-callback-err": 1,
    "no-mixed-requires": 1,
    "no-new-require": 1,
    "no-path-concat": 1,
    "no-process-exit": 0,
    "no-restricted-modules": 0,
    "no-sync": 0,
    "array-bracket-spacing": [
      1,
      "never",
      {}
    ],
    "brace-style": 1,
    "camelcase": 0,
    "comma-spacing": [
      1,
      {
        "after": true
      }
    ],
    "comma-style": 1,
    "computed-property-spacing": 0,
    "consistent-this": 1,
    "eol-last": 0,
    "func-names": 0,
    "func-style": 0,
    "id-length": 0,
    "indent": 0,
    "key-spacing": [
      1,
      {
        "afterColon": true
      }
    ],
    "lines-around-comment": 0,
    "linebreak-style": 0,
    "max-nested-callbacks": [
      1,
      4
    ],
    "new-cap": 2,
    "new-parens": 1,
    "newline-after-var": 0,
    "no-array-constructor": 2,
    "no-continue": 1,
    "no-inline-comments": 0,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multiple-empty-lines": 0,
    "no-nested-ternary": 1,
    "no-new-object": 2,
    "no-spaced-func": 1,
    "no-ternary": 0,
    "no-trailing-spaces": [
      1,
      {
        "skipBlankLines": true
      }
    ],
    "no-unneeded-ternary": 1,
    "object-curly-spacing": [
      1,
      "never",
      {}
    ],
    "one-var": 0,
    "operator-assignment": 0,
    "operator-linebreak": [
      1,
      "after"
    ],
    "padded-blocks": 0,
    "quote-props": 0,
    "quotes": [
      1,
      "double",
      "avoid-escape"
    ],
    "id-match": 0,
    "semi-spacing": 0,
    "semi": [
      1,
      "always"
    ],
    "sort-vars": 0,
    "keyword-spacing": 1,
    "space-before-blocks": [
      1,
      "always"
    ],
    "space-before-function-paren": 1,
    "space-infix-ops": 1,
    "space-return-throw-case": 0,
    "space-unary-ops": 1,
    "spaced-comment": [
      1,
      "always",
      {}
    ],
    "wrap-regex": 0,
    "arrow-parens": 0,
    "arrow-spacing": 0,
    "constructor-super": 0,
    "generator-star-spacing": 0,
    "no-class-assign": 0,
    "no-const-assign": 0,
    "no-this-before-super": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-const": 0,
    "prefer-spread": 0,
    "prefer-reflect": 0,
    "require-yield": 0,
    "max-depth": 0,
    "max-len": 0,
    "max-params": 0,
    "max-statements": 0,
    "no-bitwise": 0,
    "no-plusplus": 0
  }
}